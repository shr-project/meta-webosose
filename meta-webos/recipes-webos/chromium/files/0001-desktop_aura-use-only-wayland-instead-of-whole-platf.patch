@@ -159,7 +159,7 @@ def main(argv):
   usings = list(options.using)
 
   if options.platform_list:
-    platforms = open(options.platform_list, 'r').read().strip().split('\n')
+    platforms = ['headless']
 
   if not platforms:
     sys.stderr.write('No platforms are selected!')
